connector {
provider .sqlite
url "sqlite::memory:"
}

server {
bind ("0.0.0.0", 4015)
}

enum Sex {
    MALE
FEMALE
}

model Support {
@id @autoIncrement @readonly
id: Int
int32: Int32?
int64: Int64?
float32: Float32?
float64: Float64?
bool: Bool?
string: String?
date: Date?
dateTime: DateTime?
decimal: Decimal?
sex: Sex?
}
